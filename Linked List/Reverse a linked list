 class Node {
     int value;
    Node next;
    Node(int value) {
        this.value = value;
    }
} */

class ReverseLL
{
    // This function should reverse linked list and return
   // head of the modified linked list.
   Node reverseList(Node head)
   {
        Node prev = null;
        Node cur = head;
        
        while(cur != null)
        {
            Node temp = cur.next;
            
            cur.next = prev;
            prev = cur;
            cur = temp;
        }
        
        return prev;
   }
}
            cur = temp;
        }
        
        return prev;
   }
}
